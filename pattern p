#include <stdio.h>

int main(void) 
{
    int i,j,rows,columns;
    scanf("%d",&rows);
    scanf("%d",&columns);
    for(i=1;i<=rows;i++)
    {
    	for(j=1;j<=columns;j++)
    	{
    		if(i==1 || j==1|| i==rows/2 ||i<rows/2 && j==columns)
    		{
    			printf("*");
    		}
    		else
    		{
    			printf(" ");
    		}
    	}
    printf("\n");	
    }
    	
	return 0;
}
